<svg width="450" height="450" viewBox="0 0 450 450" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="goldGradient" cx="0.3" cy="0.3" r="0.8">
      <stop offset="0%" style="stop-color:#ffd700"/>
