  </text>
  
  <!-- Center design -->
  <text x="225" y="190" text-anchor="middle" font-family="serif" font-size="20" font-weight="bold" fill="#654321">SAINT</text>
  <text x="225" y="215" text-anchor="middle" font-family="serif" font-size="20" font-weight="bold" fill="#654321">GAUDENS</text>
  <text x="225" y="245" text-anchor="middle" font-family="serif" font-size="18" fill="#8b4513">1924</text>
  <text x="225" y="270" text-anchor="middle" font-family="serif" font-size="12" fill="#8b4513">TWENTY DOLLARS</text>
  
  <!-- Eagle silhouette -->
  <path d="M 200 160 Q 225 150 250 160 Q 240 170 225 175 Q 210 170 200 160" fill="#8b4513"/>
  
  <!-- Stars -->
  <text x="180" y="140" text-anchor="middle" font-family="serif" font-size="12" fill="#8b4513">★</text>
  <text x="270" y="140" text-anchor="middle" font-family="serif" font-size="12" fill="#8b4513">★</text>
